{
    "name": "react-themable-hoc-aphrodite-interface",
    "version": "0.1.6",
    "description": "Use Aphrodite with react-themable-hoc",
    "repository": {
        "type": "git",
        "url": "https://github.com/jtberglund/react-themable-hoc-aphrodite-interface.git"
    },
    "author": "Jeremy Berglund",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jtberglund/react-themable-hoc-aphrodite-interface/issues"
    },
    "homepage": "https://github.com/jtberglund/react-themable-hoc-aphrodite-interface",
    "keywords": [
        "react-component",
        "react",
        "react-themable-hoc",
        "theme",
        "themable",
        "css",
        "css-in-js",
        "style",
        "aphrodite",
        "interface",
        "hoc",
        "higher-order-component"
    ],
    "options": {
        "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
    },
    "types": "./index.d.ts",
    "scripts": {
        "prepublish": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
        "lint": "eslint ./src",
        "lintfix": "eslint ./src --fix",
        "testonly": "mocha $npm_package_options_mocha",
        "test": "npm run lint && npm run testonly",
        "test-watch": "npm run testonly -- --watch --watch-extensions js"
    },
    "devDependencies": {
        "aphrodite": "^2.2.0",
        "babel-cli": "^6.6.4",
        "babel-core": "^6.7.4",
        "babel-eslint": "^6.0.2",
        "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
        "babel-polyfill": "^6.7.4",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-2": "^6.5.0",
        "chai": "^3.5.0",
        "eslint": "^2.7.0",
        "eslint-plugin-babel": "^3.1.0",
        "eslint-plugin-react": "^4.2.3",
        "jsdom": "^8.1.0",
        "mocha": "^2.4.5",
        "nodemon": "^1.9.1"
    },
    "peerDependencies": {
        "aphrodite": "^2.0.0",
        "react-themable-hoc": "^0.1.2"
    },
    "dependencies": {
        "babel-runtime": "^6.6.1"
    }
}
